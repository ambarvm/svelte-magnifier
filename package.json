	"repository": {
		"type": "git",
		"url": "git+https://github.com/supercoww/svelte-magnifier.git"
	},
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
