		"eslint": "^8.5.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"postcss": "^8.4.5",
		"postcss-nesting": "^10.0.3",
		"prettier": "^2.5.1",
